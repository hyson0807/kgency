{
  "expo": {
    "name": "kgency",
    "slug": "welkit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/kgency_logo.png",
    "scheme": "kgency",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hyson.kgency",
      "infoPlist": {
        "NSUserNotificationUsageDescription": "이 앱은 면접 제안 및 지원 상태 알림을 보내기 위해 푸시 알림을 사용합니다."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/kgency_logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.hyson.kgency"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/kgency_logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/kgency_logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/kgency_logo.png",
          "color": "#ffffff",
          "sounds": []
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1d815e5e-cec3-4c35-830a-ca52c8ecd075"
      }
    },
    "owner": "welkit"
  }
}
