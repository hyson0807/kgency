{
  "expo": {
    "name": "kgency",
    "slug": "kgency",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/kgency-logo.png",
    "scheme": "kgency",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hyson.kgency"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/kgency-logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.hyson.kgency"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/kgency-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1d815e5e-cec3-4c35-830a-ca52c8ecd075"
      }
    }
  }
}
